{
  "name": "mindsize/wp-plugin-base",
  "description": "This is a sample plugin that was created with mindsize/wp-plugin-base.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0",
  "require": {
    "composer/installers": "^1.4",
    "xrstf/composer-php52": "^1.0",
    "gamajo/template-loader": "^1.3"
  },
  "require-dev": {
    "coenjacobs/mozart": "^0.2"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd",
      "\"vendor/bin/mozart\" compose"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd",
      "\"vendor/bin/mozart\" compose"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  },
  "extra": {
    "mozart": {
      "dep_namespace": "Mindsize\\WP_Plugin_Base\\Dependencies\\",
      "dep_directory": "/src/Dependencies/",
      "classmap_directory": "/includes/dependencies/",
      "classmap_prefix": "MS_WPPB_",
      "packages": [
        "gamajo/template-loader"
      ]
    }
  }
}
